# Exercise 3
print(5 < 3) # False
print(3 == 3) # True
print(3 == "3") # False
print("3" > 3) # Error
print("Hello" == "hello") # False
# Exercise 4
computer_brand = "Apple"
print(f"I have a {computer_brand} computer")
#Exercise 5
name = "Gabby"
age = 27
shoe_size = 39
info = (f"I am {name} and I am turning {age + 1} next month so I hope my boyfriend buys me a pair of shoes in size {shoe_size}")
print(info)
#Exercise 6
a = 2
b = 1
if a > b:
    print("Hello World")
#Exercise 7
user_num = int(input("Enter a number: "))
if user_num % 2 == 0:
    print("This is an even number")
else:
    print("This is an odd number")
#Exercise 8
user_name = input("What is your name? ")
if user_name == "Gabby":
    print("No, I'm Gabby")
else:
    print("I don't know you")
#Exercise 9
user_height = int(input("What is your height in cm? "))
if user_height >= 145:
    print("You are tall enough to ride!")
else:
    print("Sorry, you need to grow before you can ride")
